{
  "tasks": {
    "dev": "deno run -A --watch=static/,routes/,locales/ dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts"
  },
  "importMap": "./import_map.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "fmt": {
    "singleQuote": true,
    "semiColons": false
  }
}
